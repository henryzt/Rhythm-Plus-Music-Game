{
  "name": "rhythm-plus-game",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "author": {
    "name": "henryz00"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.js\" && eslint --fix \"src/**/*.js\"",
    "lint": "eslint --fix \"src/**/*.js\"",
    "start": "NODE_ENV=development webpack-dev-server --mode development --progress --history-api-fallback",
    "build:dev": "NODE_ENV=development webpack --mode development",
    "build": "NODE_ENV=production webpack --mode production",
    "clean": "rm -rf ./dist",
    "serve": "yarn run build && serve .",
    "deploy": "yarn build && firebase deploy && git push"
  },
  "dependencies": {
    "animate.css": "^4.1.0",
    "axios": "^0.19.2",
    "countup.js": "^2.0.5",
    "firebase": "^7.15.5",
    "firebaseui": "^4.5.1",
    "hammerjs": "^2.0.8",
    "howler": "^2.2.0",
    "js-md5": "^0.7.3",
    "vanilla-tilt": "^1.7.0",
    "vue": "^2.6.11",
    "vue-awesome": "^4.0.2",
    "vue-countup-v2": "^4.0.0",
    "vue-router": "^3.3.4",
    "vue-slider-component": "^3.1.5",
    "vue-spinner": "^1.0.3",
    "vue-youtube": "^1.4.0",
    "vue2-circle-progress": "^1.2.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2020": "^1.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css,json}": [
      "prettier --write"
    ]
  },
  "license": "ISC"
}
